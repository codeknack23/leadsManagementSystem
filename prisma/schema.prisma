generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"  // SQLite database file
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
}

model Lead {
  id     Int    @id @default(autoincrement())
  name   String
  email  String
  phone  String
  status String
}
